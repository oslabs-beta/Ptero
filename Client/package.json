    "svelte-preprocess": "^4.9.4",
    "sveltestrap": "^5.6.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "@sveltejs/pancake": "^0.0.18",
    "bootstrap": "^5.1.3",
    "electron": "^12.0.9",
    "electron-builder": "^22.10.5",
    "pancake": "^3.4.1",
    "svelte-kit": "^1.0.0",
    "@sveltejs/adapter-static": "^1.0.0-next.11"
